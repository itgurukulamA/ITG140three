package com.example.demo.Jdbcpojo.njdbc;

import java.util.HashMap;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.namedparam.NamedParameterJdbcTemplate;
import org.springframework.stereotype.Service;

@Service

public class NamedjdbcServices {
	
@Autowired	
	NamedParameterJdbcTemplate tem;

	
   public String insert(Njdbc jdbc) {
	   

		String b="insert into ece(id,name,age,college)values (:id,:name,:age,:college)";  
		 return tem.update(b,jdbc.getId(),jdbc.getName(),jdbc.getAge()jdbc.getCollege());
		 
			
}
}  
	


