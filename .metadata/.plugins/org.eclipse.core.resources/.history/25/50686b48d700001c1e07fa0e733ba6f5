package collection;

import java.util.List;

 class ArrayListex {
	public static void main (String args[]) {
		ArrayListex list=new ArrayListex();  
		list.("Ravi");  
		list.add("Vijay");  
		list.add("Ravi");  
		list.add("Ajay");  
		//Traversing list through Iterator  
		Iterator itr=list.iterator();  
		while(itr.hasNext()){  
		System.out.println(itr.next());  
		}  
		}  
	}



 

}
